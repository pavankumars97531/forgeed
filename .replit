modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true
integrations = ["flask_authentication:1.0.0", "python_openai:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ForgeEd Flask App"

[[workflows.workflow]]
name = "ForgeEd Flask App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36701
externalPort = 3002

[[ports]]
localPort = 40401
externalPort = 3000

[[ports]]
localPort = 43471
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind=0.0.0.0:5000", "--workers=2", "--timeout=120", "app:app"]
